{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "editor.formatOnSave": true,
  "yaml.format.enable": true,
  "ruby.rubocop.useBundler": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#af42d5",
    "activityBar.activeBorder": "#d1a72e",
    "activityBar.background": "#af42d5",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#d1a72e",
    "activityBarBadge.foreground": "#15202b",
    "editorGroup.border": "#af42d5",
    "panel.border": "#af42d5",
    "sideBar.border": "#af42d5",
    "statusBar.background": "#9529bb",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#af42d5",
    "titleBar.activeBackground": "#9529bb",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#9529bb99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#9529bb"
